{
  // bot 相关配置
  "bot": {
    "debug": false,                                     // 是否启用调试模式
    "debugConfig": {},                                  // 调试模式配置
    "saveMedia": false,                                 // 是否保存图片等媒体文件
    "owner": ""                                         // bot管理员
  },
  // 登录相关配置
  "login": {
    "autoLogin": true,                                  // 是否启用自动登录
    "printQrcode": true,                                // 是否打印二维码
    "retryCount": 3,                                    // 重试次数
    "encryptLoginInfo": true,                           // 是否加密登录信息
    "encryptKey": "jbot"                                // 加密密钥
  },
  // 命令相关配置
  "command": {
    "showTip": true,                                    // 是否显示命令提示
    "noPermissionTip": "你配执行这个命令吗？"               // 无权限提示
  },
  // 适配器相关配置
  "adapter": {
    "name": "x.ovo.jbot.WebAdapter"
  }
}